@model IEnumerable<CSharpDay2.Models.Homes>
@{
    ViewBag.Title = "Home Page";
}

<table class="table-bordered table-striped">
    <tr>
        <th>Id</th>
        <th>Address</th>
        <th>BedRooms</th>
    </tr>



    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Address</td>
            <td>@item.BedRooms</td>

        </tr>
    }

</table>