@model IEnumerable<CSharpDay2.Models.Car>
@{
    ViewBag.Title = "Cars";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<table class="table-bordered table-striped">
    <tr>
        <th>Id</th>
        <th>Make</th>
        <th>Year</th>
    </tr>



    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Make</td>
            <td>@item.Year</td>

        </tr>
    }

</table>